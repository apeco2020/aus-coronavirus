issn = {00218901},
journal = {Journal of Applied Ecology},
keywords = {Cats,Invasive species,Rabbits,Sub-Antarctic,Trophic cascade},
number = {1},
pages = {73--81},
title = {{Indirect effects of invasive species removal devastate World Heritage Island}},
volume = {46},
year = {2009}
}
