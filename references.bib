@article{Boutin1995c,
abstract = {Fluctuating populations of small mammals provide an excellent opportunity to study the functional and numerical responses of predators because of the wide range in prey density that occurs. I reinterpret data from six studies that have examined the role of predation in the population dynamics of voles in California, southern Sweden and western Finland, of snowshoe hares in northern Canada, and of house mice and rabbits in Australia. Most studies have measured functional responses by relying on changes in diet as reflected by scat or stomach contents. These methods are probably biased toward showing predator satiation. Contrary to previous conclusions I find that there is little evidence for non-linear (Type 111) functional-response curves or predator satiation at high prey densities. Recent studies indicate that the functional and numerical responses of predators can be rapid and strong enough to initiate cyclic declines, dampen fluctuations, or even cause stable numbers. The exception to this appears to be the irruptions of mice and rabbits in Australia. I propose a general explanation for the role of predation whereby the effect of predation is largely dependent on the entire prey community. When potentially cyclic prey are a small component of the overall prey biomass, generalist predators are able to prevent fluctuations by strong functional or numerical responses. As the prey community becomes dominated by a few species that fluctuate, limit cycles predominate. Limit cycles turn into irruptive population dynamics when seasonal prey reproduction is eliminated because of extended periods of vegetation growth (vegetation flushes following drought). In the future we must test assumptions underlying the way we study predation by telemetric monitoring of prey mortality and by experimentally manipulating predation.},
author = {Boutin, Stan},
journal = {Wildl. Res},
pages = {89--100},
title = {{Testing Predator-Prey Theory by Studying Fluctuating Populations of Small Mammals}},
volume = {22},
year = {1995}
}
@article{StClair2011,
abstract = {The widespread invasive rodents Rattus norvegicus, R. rattus, R. exulans and Mus musculus have been implicated in the decline and extinction of hundreds of island endemic vertebrates, but their effects on island invertebrates are less well-known. Here I present the first global review of the subject, which confirms that large-bodied invertebrates are most at risk from these rodents, and that although a disproportionate number of studies (69{\%}) are from New Zealand, rodent-invertebrate impacts are geographically widespread. Mechanisms of impact are both direct (mediated by predation) and indirect (involving intermediary species). Some studies also suggest knock-on effects on ecosystem properties, and given the diverse ecological functions of invertebrates (as detritivores, primary consumers, predators, prey and pollinators), I suggest that an understanding of the interactions between invasive rodents and invertebrates in island ecosystems is essential for effective conservation management. Currently many reported impacts are unquantified, come from uncontrolled and unreplicated designs, or rely on time-series with inadequate baseline data. In addition to basic improvements in study design, this review highlights a need for studies which investigate mechanisms of impact, or impacts across trophic levels. {\textcopyright} 2010 Elsevier Ltd.},
author = {{St Clair}, James J.H.},
doi = {10.1016/j.biocon.2010.10.006},
isbn = {0006-3207},
issn = {00063207},
journal = {Biological Conservation},
keywords = {Invasive species,Invertebrate,Mus,New Zealand,Rattus,Rodents,Trophic cascade,Weta},
number = {1},
pages = {68--81},
title = {{The impacts of invasive rodents on island invertebrates}},
volume = {144},
year = {2011}
}
