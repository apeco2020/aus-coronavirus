@article{Wanless2007,
abstract = {The house mouse, Mus musculus, is one of the most widespread and well-studied invasive mammals on islands. It was thought to pose little risk to seabirds, but video evidence from Gough Island, South Atlantic Ocean shows house mice killing chicks of two IUCN-listed seabird species. Mouse-induced mortality in 2004 was a significant cause of extremely poor breeding success for Tristan albatrosses, Diomedea dabbenena (0.27 fledglings/pair), and Atlantic petrels, Pterodroma incerta (0.33). Population models show that these levels of predation are sufficient to cause population decreases. Unlike many other islands, mice are the only introduced mammals on Gough Island. However, restoration programmes to eradicate rats and other introduced mammals from islands are increasing the number of islands where mice are the sole alien mammals. If these mouse populations are released from the ecological effects of predators and competitors, they too may become predatory on seabird chicks.},
annote = {mice island predators. important reference},
author = {Wanless, Ross M and Angel, Andrea and Cuthbert, Richard J and Hilton, Geoff M and Ryan, Peter G},
doi = {10.1098/rsbl.2007.0120},
file = {::},
journal = {Biol. Lett},
keywords = {Important,direct reference},
mendeley-tags = {Important,direct reference},
pages = {241--244},
title = {{Can predation by invasive mice drive seabird extinctions?}},
volume = {3},
year = {2007}
}
@article{Miller2001,
abstract = {The home range of stoats (Mustela erminea) was determined as part of a programme to protect Okarito brown kiwi chicks (Apteryx australis) 'Okarito', from predation. Twenty-seven stoats were fitted with radio-transmitters and tracked in two podocarp (Podocarpaceae) forests, in south Westland, New Zealand, from July 1997 to May 1998. Home-range area was determined for 19 animals by minimum convex polygons and restricted-edge polygons, and core areas were determined by hierarchical cluster analysis. The mean home ranges of males across all seasons calculated by minimum convex polygon (210 +/- 28 ha (+/- s.e.)) and restricted-edge polygon (176 +/- 29 ha) were significantly larger than those of females across all seasons (89 +/- 14 ha and 82 +/- 12 ha). The mean home range of males calculated by minimum convex polygon during the breeding season (256 +/- 38 ha) was significantly larger than the mean home range pooled across the non-breeding seasons (149 +/- 16 ha), whereas that calculated by restricted-edge polygon was not significantly different. The mean home range of females during the breeding season was not significantly different from that in the non-breeding seasons when estimated by either method. Overlap of home ranges was observed within and between sexes in all seasons, with the greatest proportion of home range overlap being male-female. The mean home range of females in spring and summer is used to guide the spacing of control stations.},
author = {Miller, C. and Elliot, M. and Alterio, N.},
doi = {10.1071/WR00001},
isbn = {1035-3712},
issn = {10353712},
journal = {Wildlife Research},
number = {2},
pages = {165--172},
pmid = {12747},
title = {{Home range of stoats (Mustela erminea) in podocarp forest, south Westland, New Zealand: Implications for a control strategy}},
volume = {28},
year = {2001}
}
@article{Moon2015,
abstract = {Most successful control or eradication programs have been implemented in$\backslash$nuninhabited regions, where little or no consideration of socio-political$\backslash$nfactors has been required. A considerable challenge for invasion$\backslash$nscientists and decision-makers lies in controlling and eradicating$\backslash$nspecies from inhabited regions, where challenges related to community$\backslash$nstakeholder perceptions of the need to act, animal rights and$\backslash$nuncertainty surrounding the presence and abundance of invasive species$\backslash$ncan threaten program development and implementation. We interviewed$\backslash$nsenior policy advisors and scientists who developed and implemented$\backslash$nEuropean red fox eradication policies on the inhabited island state of$\backslash$nTasmania, Australia. Our aim was to understand how they perceived that$\backslash$nscientific evidence and community stakeholders' knowledge and$\backslash$npreferences should be involved in developing and implementing policy for$\backslash$nfox detection and eradication. Respondents perceived that scientific$\backslash$nevidence was best used in determining the need to act, assessing risks,$\backslash$ndesigning eradication operations and monitoring progress. Overall, they$\backslash$nperceived that involving community stakeholders was important in$\backslash$ncreating socially acceptable eradication operations that enable access$\backslash$nto private land to implement eradication options. Community$\backslash$nstakeholders' knowledge was perceived, largely by scientists, to be$\backslash$nbased on misinformation, misconceptions and mistrust in science and,$\backslash$ntherefore, of limited value in informing policy. Community stakeholders$\backslash$nwere therefore not viewed as equal partners in the policy-making process$\backslash$nby scientists. Only those stakeholders who performed a clear function$\backslash$nwere considered valuable. We propose that increased likelihood of$\backslash$nsuccessful eradication of invasive species from inhabited areas can be$\backslash$nachieved by using a functional approach to stakeholder inclusion, to$\backslash$nprevent dissent and conflict by legitimizing varied knowledge and$\backslash$ncapacity, leading to co-production of knowledge within a co-management$\backslash$ngovernance system.},
author = {Moon, Katie and Blackman, Deborah A. and Brewer, Tom D.},
doi = {10.1007/s10530-015-0904-5},
file = {:C$\backslash$:/Users/s435389/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Moon, Blackman, Brewer - 2015 - Understanding and integrating knowledge to improve invasive species management.pdf:pdf},
isbn = {1053001509},
issn = {15731464},
journal = {Biological Invasions},
keywords = {Australia,Co-management,Co-production,Functional typology,Qualitative research,Stakeholders},
number = {9},
pages = {2675--2689},
publisher = {Springer International Publishing},
title = {{Understanding and integrating knowledge to improve invasive species management}},
volume = {17},
year = {2015}
}
