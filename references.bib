file = {::},
isbn = {1469-8137 (Electronic)
0028-646X (Linking)},
issn = {14698137},
journal = {New Phytologist},
keywords = {Moran effect,economy of scale,masting behaviour,pollen coupling,pollen limitation,resource budget,resource matching,variable seed production},
