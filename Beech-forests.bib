	author = {O'Donnell, C F J and Dilks, P J and Elliott, G P},
	year = {1996},
	keywords = {breed-, cephala, fenn trapping, ing success, mohoua ochro-, mohua, mustela erminea, predation, productivity, stoats, yellowhead},
