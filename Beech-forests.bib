@article{hein_recovery_2015,
	title = {Recovery of small rodent populations after population collapse},
	volume = {42},
	issn = {1448-5494},
	doi = {10.1071/WR14165},
	abstract = {In this review we summarised published knowledge about small mammal population recovery in re-gard to sudden population collapse of small mammal species regardless if it is caused by natural or man-made events. We determined recovery mechanisms, recovery time and recovery rate to make suggestions how to adapt and optimise current Â¬methods for the regulation of small mammal population size for pest management and for conservation. It is vital to know the principles of recovery mechanisms for pest control and conservation to align management methods either to sustainably keep animal abundance at a permanent minimum level or to increase population size. Collapses can be caused naturally as in the decline phase of multi-annual fluctuations and after natu-ral disasters or by man-made events such as pesticide applications. In general, there are two ways for recovery to take place: in situ survival and multiplication of a small remaining fraction of the population, immigration, or a combination thereof. The recovery mechanism strongly depends on life-history strategy, social behaviour and density-dependent processes in population dynamics of the species in question. Also, the kind of disturbance, its intensity and spatial scale as well as environmental circumstances such as the presence and distance of refuge areas have to be taken into account. Recovery time can vary from a couple of days to several years, depending on the speciesâ reproductive potential, and the kind of disturbances no matter whether collapse is man-made or naturally. Ultimately, most populations rebound to levels equal to pre-collapse numbers. Based on present knowledge, case-by-case decisions seem appropriate for small scale conservation action. For pest control operation a large scale approach seems necessary. Further research activities are required to make sound species specific recommendations.},
	number = {2},
	journal = {Wildlife Research},
	author = {Hein, S. and Jacob, J.},
	year = {2015},
	keywords = {conservation, migration, pest control, pest management, re-colonisation, rodenticides.},
	pages = {108--118}
}

