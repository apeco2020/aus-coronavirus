@article{krebs_density_2011,
	title = {Density estimation for small mammals from livetrapping grids: rodents in northern {Canada}},
	volume = {92},
	issn = {0022-2372},
	doi = {10.1644/10-MAMM-A-313.1},
	abstract = {Management agencies and quantitative ecologists need robust estimates of population density. The best way of converting population estimates of livetrapped small mammals to population density is not clear. We estimated population density on livetrapping grids with 4 estimators applied to 3 species of boreal forest and 3 species of tundra rodents to test for relative differences in density estimators. We used 2 spatial estimators proposed by Efford (2009) and 2 traditional boundary-strip estimators designed for grid livetrapping. We analyzed mark–recapture data from 104 trapping sessions from the boreal forest at Kluane, Yukon (n = 4,818 individuals), and 56 trapping sessions from tundra areas of Herschel Island and Komakuk Beach in northern Yukon (n = 1,327 individuals). For boreal forest rodents on average both boundary-strip methods produced density estimates larger than Efford's maximum-likelihood (ML) estimator by as much as 50\% at all population densities up to 25 animals/ha. For tundra rodents both boundary-strip methods produced density estimates smaller than Efford's ML at low density ({\textless}1.5/ha) and larger than Efford's ML density by 36–63\% at high density (25/ha). Efford's inverse prediction estimator produced larger density estimates than the ML estimator by 4\% for the boreal forest and 32\% for the tundra rodents. Relationships were high between all the estimators, such that trends in density could be inferred from all methods. Determining the bias in population density estimators in small mammals will require data from populations spatially closed and completely enumerated. For our small mammals Efford's ML estimator typically provided density estimates smaller than those produced by conventional boundary-strip estimators.},
	number = {5},
	journal = {Journal of Mammalogy},
	author = {Krebs, Charles J. and Boonstra, Rudy and Gilbert, Scott and Reid, Donald and Kenney, Alice J. and Hofer, Elizabeth J.},
	year = {2011},
	keywords = {1, 10, 10-mamm-a-313, 1644, density 4, density estimation, dicrostonyx groenlandicus, doi, e 2011 american society, lemmus sibiricus, microtus, myodes rutilus, of mammalogists, peromyscus maniculatus, yukon},
	pages = {974--981}
}

@article{moon_understanding_2015,
	title = {Understanding and integrating knowledge to improve invasive species management},
	volume = {17},
	issn = {1053001509},
	doi = {10.1007/s10530-015-0904-5},
	abstract = {Most successful control or eradication programs have been implemented in{\textbackslash}nuninhabited regions, where little or no consideration of socio-political{\textbackslash}nfactors has been required. A considerable challenge for invasion{\textbackslash}nscientists and decision-makers lies in controlling and eradicating{\textbackslash}nspecies from inhabited regions, where challenges related to community{\textbackslash}nstakeholder perceptions of the need to act, animal rights and{\textbackslash}nuncertainty surrounding the presence and abundance of invasive species{\textbackslash}ncan threaten program development and implementation. We interviewed{\textbackslash}nsenior policy advisors and scientists who developed and implemented{\textbackslash}nEuropean red fox eradication policies on the inhabited island state of{\textbackslash}nTasmania, Australia. Our aim was to understand how they perceived that{\textbackslash}nscientific evidence and community stakeholders' knowledge and{\textbackslash}npreferences should be involved in developing and implementing policy for{\textbackslash}nfox detection and eradication. Respondents perceived that scientific{\textbackslash}nevidence was best used in determining the need to act, assessing risks,{\textbackslash}ndesigning eradication operations and monitoring progress. Overall, they{\textbackslash}nperceived that involving community stakeholders was important in{\textbackslash}ncreating socially acceptable eradication operations that enable access{\textbackslash}nto private land to implement eradication options. Community{\textbackslash}nstakeholders' knowledge was perceived, largely by scientists, to be{\textbackslash}nbased on misinformation, misconceptions and mistrust in science and,{\textbackslash}ntherefore, of limited value in informing policy. Community stakeholders{\textbackslash}nwere therefore not viewed as equal partners in the policy-making process{\textbackslash}nby scientists. Only those stakeholders who performed a clear function{\textbackslash}nwere considered valuable. We propose that increased likelihood of{\textbackslash}nsuccessful eradication of invasive species from inhabited areas can be{\textbackslash}nachieved by using a functional approach to stakeholder inclusion, to{\textbackslash}nprevent dissent and conflict by legitimizing varied knowledge and{\textbackslash}ncapacity, leading to co-production of knowledge within a co-management{\textbackslash}ngovernance system.},
	number = {9},
	journal = {Biological Invasions},
	author = {Moon, Katie and Blackman, Deborah A. and Brewer, Tom D.},
	year = {2015},
	keywords = {Australia, Co-management, Co-production, Functional typology, Qualitative research, Stakeholders},
	pages = {2675--2689},
	file = {Moon, Blackman, Brewer_2015_Understanding and integrating knowledge to improve invasive species management:C\:\\zotero-backup-april2019\\storage\\NZ42GN6Y\\Moon, Blackman, Brewer_2015_Understanding and integrating knowledge to improve invasive species management.pdf:application/pdf;Moon, Blackman, Brewer_2015_Understanding and integrating knowledge to improve invasive species management:C\:\\zotero-backup-april2019\\storage\\YDGPW93C\\Moon, Blackman, Brewer_2015_Understanding and integrating knowledge to improve invasive species management.pdf:application/pdf;Moon, Blackman, Brewer_2015_Understanding and integrating knowledge to improve invasive species management:C\:\\zotero-backup-april2019\\storage\\LCAHQIXY\\Moon, Blackman, Brewer_2015_Understanding and integrating knowledge to improve invasive species management.pdf:application/pdf;Moon, Blackman, Brewer_2015_Understanding and integrating knowledge to improve invasive species management:C\:\\zotero-backup-april2019\\storage\\VZPJJV5B\\Moon, Blackman, Brewer_2015_Understanding and integrating knowledge to improve invasive species management.pdf:application/pdf;Moon, Blackman, Brewer_2015_Understanding and integrating knowledge to improve invasive species management:C\:\\zotero-backup-april2019\\storage\\CUJ63WTI\\Moon, Blackman, Brewer_2015_Understanding and integrating knowledge to improve invasive species management.pdf:application/pdf}
}

@article{monks_testing_2006,
	title = {Testing the resource-matching hypothesis in the mast seeding tree {Nothofagus} truncata ({Fagaceae})},
	volume = {31},
	issn = {1442-9993},
	doi = {10.1111/j.1442-9993.2006.01565.x},
	abstract = {The genus Nothofagus in New Zealand and Australia exhibits strong mast seeding (i.e. highly variable seed crops between years). Seed crop variation is synchronized within and between species over large spatial scales, and results in greatly increased wind pollination efficiency which could provide a selective benefit favouring the maintenance of mast seeding. However, the null hypothesis (that plants simply match their reproductive effort to the variable resources available each year) has not been tested in Nothofagus. Here we use a 33-year dataset on seedfall and wood ring increments for 19 individual Nothofagus truncata trees at Orongorongo, New Zealand, to test for the presence of switching (exaggeration of seedfall variability by diverting resources into, then out of, reproduction). A generalized least squares model explained 40.7\% of the variance in standardized ring widths, using six weather variables (absolute minimum temperatures in March (lag 0) April (lag 0 and lag 1), May (lag 0) and rainfall in November and February (lag 0) ) and seedfall. Seedfall had a negative relationship with the current year's ring widths even after controlling for all significant weather variables. This shows that switching is occurring in N. truncata within individuals among years, and therefore that masting in this species is the result of selective forces such as increased wind pollination efficiency. As this result has been demonstrated for very few masting species, we call for this test to be applied more widely.},
	number = {3},
	journal = {Austral Ecology},
	author = {Monks, Adrian and Kelly, Dave},
	year = {2006},
	keywords = {Annual ring growth, Climate, Economies of scale, Switching, Wind pollination},
	pages = {366--375}
}

@article{seefeld_statistics_2007,
	title = {Statistics {Using} {R} with {Biological} {Examples}},
	issn = {9780132391153},
	url = {http://cran.r-project.org/doc/contrib/Seefeld_StatsRBio.pdf},
	abstract = {This book is a manifestation of my desire to teach researchers in biology a bit more about statistics than an ordinary introductory course covers and to introduce the utilization of R as a tool for analyzing their data. My goal is to reach those with little or no training in higher level statistics so that they can do more of their own data analysis, communicate more with statisticians, and appreciate the great potential statistics has to offer as a tool to answer biological questions. This is necessary in light of the increasing use of higher level statistics in biomedical research. I hope it accomplishes this mission and encourage its free distribution and use as a course text or supplement. I thank all the teachers, professors, and research colleagues who guided my own learning especially those in the statistics and biological research departments at the University of Michigan, Michigan State University, Dartmouth Medical School, and the University of New Hampshire. I thank the Churchill group at the Jackson labs to invite me to Bar Harbor while I was writing the original manuscript of this book. I especially thank Ernst Linder for reviewing and working with me on this manuscript, NHCTC for being a great place to teach, and my current colleagues at Tufts-NEMC. I dedicate this work to all my students past, present and future both those that I teach in the classroom and the ones I am teaching through my writings. I wish you success in your endeavors and encourage you never to quit your quest for answers to the research questions that interest you most.},
